# Agent Configuration
AGENT_NAME=NDA Agent
AGENT_DESCRIPTION=AI agent for NDA document management
DEBUG_MODE=False
VERBOSE_LOGGING=False

# Get your API key from: https://developers.pandadoc.com/reference/authorization
